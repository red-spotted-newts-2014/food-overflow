<div class="login_holder">
  <%= form_tag sessions_path do%>
  <p>
    <%= text_field_tag :email, params[:email], placeholder: "email", autofocus: true %>
  </p>
  <p>
  <%= password_field_tag :password, nil, placeholder: "password" %>
  </p>
  <p>
    <%= button_tag "Sign In" %>
  </p>
  <% end %>
</div>